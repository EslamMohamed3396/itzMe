<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout7"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragment.changePassword.ChangePasswordFragment">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar_back"
        android:layout_width="0dp"
        android:layout_height="?android:attr/actionBarSize"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:src="@drawable/ic_email_call"
        app:layout_constraintBottom_toTopOf="@+id/guideline4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/guideline4" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.3" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="24dp"
        android:fontFamily="@font/cairo"
        android:text="Please enter the code , the code has been sent to your e-mail"
        android:textSize="12sp"
        app:layout_constraintBottom_toTopOf="@+id/guideline4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView16"
        app:layout_constraintTop_toTopOf="@+id/guideline4" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/cairo_bold"
        android:text="mohmmed@gmail.com"
        android:textAlignment="center"
        android:textColor="@color/dark_blue"
        android:textSize="12sp"
        app:layout_constraintEnd_toEndOf="@+id/textView20"
        app:layout_constraintStart_toStartOf="@+id/textView20"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/stroke_password"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView21">

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/Confirm_Code_one_InputLayout"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="16dp"
            app:boxBackgroundColor="@color/gray"
            app:boxCornerRadiusBottomEnd="@dimen/radius_8"
            app:boxCornerRadiusBottomStart="@dimen/radius_8"
            app:boxCornerRadiusTopEnd="@dimen/radius_8"
            app:boxCornerRadiusTopStart="@dimen/radius_8"
            app:hintEnabled="false"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/Confirm_Code_two_InputLayout"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/cairo"
                android:gravity="center"
                android:inputType="numberDecimal"
                android:lines="1"
                android:maxLength="1"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                tools:text="1" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/Confirm_Code_two_InputLayout"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginEnd="8dp"
            app:boxBackgroundColor="@color/gray"
            app:boxCornerRadiusBottomEnd="@dimen/radius_8"
            app:boxCornerRadiusBottomStart="@dimen/radius_8"
            app:boxCornerRadiusTopEnd="@dimen/radius_8"
            app:boxCornerRadiusTopStart="@dimen/radius_8"
            app:hintEnabled="false"
            app:layout_constraintBottom_toBottomOf="@+id/Confirm_Code_one_InputLayout"
            app:layout_constraintEnd_toStartOf="@+id/Confirm_Code_three_InputLayout"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toEndOf="@+id/Confirm_Code_one_InputLayout"
            app:layout_constraintTop_toTopOf="@+id/Confirm_Code_one_InputLayout">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/cairo"
                android:gravity="center"
                android:inputType="numberDecimal"
                android:lines="1"
                android:maxLength="1"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                tools:text="2" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/Confirm_Code_three_InputLayout"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginEnd="8dp"
            app:boxBackgroundColor="@color/gray"
            app:boxCornerRadiusBottomEnd="@dimen/radius_8"
            app:boxCornerRadiusBottomStart="@dimen/radius_8"
            app:boxCornerRadiusTopEnd="@dimen/radius_8"
            app:boxCornerRadiusTopStart="@dimen/radius_8"
            app:hintEnabled="false"
            app:layout_constraintBottom_toBottomOf="@+id/Confirm_Code_two_InputLayout"
            app:layout_constraintEnd_toStartOf="@+id/Confirm_Code_four_InputLayout"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toEndOf="@+id/Confirm_Code_two_InputLayout"
            app:layout_constraintTop_toTopOf="@+id/Confirm_Code_two_InputLayout">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/cairo"
                android:gravity="center"
                android:inputType="numberDecimal"
                android:lines="1"
                android:maxLength="1"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                tools:text="3" />
        </com.google.android.material.textfield.TextInputLayout>

        <com.google.android.material.textfield.TextInputLayout
            android:id="@+id/Confirm_Code_four_InputLayout"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:layout_marginEnd="16dp"
            app:boxBackgroundColor="@color/gray"
            app:boxCornerRadiusBottomEnd="@dimen/radius_8"
            app:boxCornerRadiusBottomStart="@dimen/radius_8"
            app:boxCornerRadiusTopEnd="@dimen/radius_8"
            app:boxCornerRadiusTopStart="@dimen/radius_8"
            app:hintEnabled="false"
            app:layout_constraintBottom_toBottomOf="@+id/Confirm_Code_three_InputLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintStart_toEndOf="@+id/Confirm_Code_three_InputLayout"
            app:layout_constraintTop_toTopOf="@+id/Confirm_Code_three_InputLayout">

            <com.google.android.material.textfield.TextInputEditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fontFamily="@font/cairo"
                android:gravity="center"
                android:inputType="numberDecimal"
                android:lines="1"
                android:maxLength="1"
                android:textColor="@color/black"
                android:textSize="18sp"
                android:textStyle="bold"
                tools:text="4" />
        </com.google.android.material.textfield.TextInputLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/cairo_bold"
        android:text="Resend the code"
        android:textAlignment="center"
        android:textColor="@color/dark_blue"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/join_now_btn"
        style="@style/MaterialButtonStyle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        android:text="Confirm"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView22"
        app:layout_constraintVertical_bias="1.0" />

</androidx.constraintlayout.widget.ConstraintLayout>